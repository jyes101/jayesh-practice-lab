import java.io.*;
import java.math.*;
class golden_Tringle{
  public static void main(String args[])throws IOException{
    int no_of_boxes;
    int no_of_rubber_bands;
    int no_of_maximum_boxes;
    int length_of_boxes[] = new int[10];
    int original_radius_of_rubber_band[] = new int[10];
    int max_radius_of_rubber_band[] = new int[10];
    int i,j,k;
    
    DataInputStream in = new DataInputStream(System.in);
    System.out.println("Enter Number of Boxes:");
    no_of_boxes = Integer.parseInt(in.readLine());
    
    System.out.println("Enter lenght of the boxes:");
    for(i=0;i<no_of_boxes;i++){
    	length_of_boxes[i] = Integer.parseInt(in.readLine());
    	System.out.print("\n");
    	System.out.print(lenght_of_boxes[i]);
    	}
    
    	System.out.println("Enter number of rubber band:");
    	no_of_rubber_bands = no_of_boxes;
    	System.out.println("Number of rubber_bands:->" + no_of_rubber_bands);
    
    	System.out.println("Enter original & maximum radius of the rubber_bands:");
    
    	for(j=0;j<no_of_rubber_bands;j++){
    		original_radius_of_rubber_band[j] = Integer.parseInt(in.readLine());
    		max_radius_of_rubber_band[j] = Integer.parseInt(in.readLine());
    		System.out.print("\n");
    		System.out.print(original_radius_of_rubber_band[j] + " " + max_radius_of_rubber_band[j]);
    		}
    		
    	while(no_of_boxes != 0){
    	int count = 0;
    		for(k=0;k<no_of_boxes;k++){
    			float origianl_circumference_of_rubber_band = 2*Math.PI*original_radius_of_rubber_band[k];
    			float maximum_circumference_of_rubber_band = 2*Math.PI*max_radius_of_rubber_band[k];
    			int circumference_of_box = 4*length_of_boxes[k];
    			
    			if(origianl_circumference_of_rubber_band <= circumference_of_box){
    				if(circumference_of_box <= maximum_circumference_of_rubber_band){
    					count++;
    					}
    				else{
    				System.out.println("Opps! there is something wrong.");
    				}
    			else{
    				System.out.println("Opps! there is something wrong.");
    			}
    		}
    		System.out.println("Maximum number of box/boxes is/are paked" + count);
    	}
    	System.out.println("Opps! You should give valid number of boxes:");
    }
   }
    					
    			
    			
    			
    		
    		
    	
    
    
    
    
    
    
